@model List<AddressListViewModel>

<table class="table table-stripped">

    <tr>
        <th>Şehir</th>
        <th>Adres</th>
        <th>PostaKodu</th>
        <th></th>

    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Sehir</td>
            <td>@item.Adres </td>
            <td>@item.PostaKodu </td>
        </tr>
    }



</table>